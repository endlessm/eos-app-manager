# Add additional XDG_DATA_DIRS path.

EAM_XDG_DATA_PREFIX="/endless/share"
XDG_DATA_DIRS=${XDG_DATA_DIRS}:${EAM_XDG_DATA_PREFIX}
export XDG_DATA_DIRS

EAM_BIN_PREFIX="/endless/bin"
PATH=${PATH}:${EAM_BIN_PREFIX}
export PATH
